/* pages/contract-list/contract-list.wxss */
.container{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}
.contract_listBox{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.item{
  width: 95%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f2f2f2;
}
.item:first-child{
  margin-top: 20rpx;
}
.item_texta{
  font-size: 28rpx;
  display: flex;
  align-items: center;
}

.no_more{
  width: 100%;
  font-size: 26rpx;
  color: #ccc;
  text-align: center;
  margin-top: 50rpx;
}